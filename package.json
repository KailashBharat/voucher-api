  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon",
    "prod": "yarn build && node dist/index.js",
    "test": "jest",
    "docker:mysql": "dotenv -- cross-var docker run -d --name voucher-mysql -p %DB_PORT%:3306 -e MYSQL_ROOT_PASSWORD=%DB_ROOT_PASSWORD% mysql:latest",
    "docker:postgres": "dotenv -- cross-var docker run -d --name voucher-postgres -p %DB_PORT%:5432 -e POSTGRES_PASSWORD=%DB_PASSWORD% -e POSTGRES_USER=%DB_USER% -e POSTGRES_DB=%DB_DATABASE% postgres:latest"
  },
